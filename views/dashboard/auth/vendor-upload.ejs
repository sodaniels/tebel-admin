<!DOCTYPE html>

<html class="loading" lang="en" data-textdirection="ltr">
<!-- BEGIN: Head-->

<head>
    <meta http-equiv="Content-Type" content="text/html; charset=UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0, user-scalable=0, minimal-ui">
    <meta name="description" content="">
    <meta name="keywords" content="">
    <meta name="author" content="">
    <title>
        <%= pageTitle %>
    </title>

    <link rel="shortcut icon" type="image/x-icon" href="">
    <link
        href="https://fonts.googleapis.com/css?family=Open+Sans:300,300i,400,400i,600,600i,700,700i%7CQuicksand:300,400,500,700"
        rel="stylesheet">

    <!-- BEGIN: Vendor CSS-->
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/vendors.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/icheck.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/custom.css">
    <!-- END: Vendor CSS-->

    <!-- BEGIN: Theme CSS-->
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/bootstrap.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/bootstrap-extended.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/css/colors.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/components.min.css">
    <link rel="stylesheet" href="https://cdn.jsdelivr.net/npm/line-awesome/dist/line-awesome/css/line-awesome.min.css">
    <!-- END: Theme CSS-->

    <!-- BEGIN: Page CSS-->
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/horizontal-menu.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/palette-gradient.min.css">
    <link rel="stylesheet" type="text/css" href="/assets/dashboard/css/login-register.min.css">
    <!-- END: Page CSS-->

</head>
<!-- END: Head-->

<!-- BEGIN: Body-->

<body class="horizontal-layout horizontal-menu horizontal-menu-padding 1-column   blank-page" data-open="click"
    data-menu="horizontal-menu" data-col="1-column">
    <!-- BEGIN: Content-->
    <div class="app-content container center-layout mt-2">
        <div class="content-overlay"></div>
        <div class="content-wrapper">
            <div class="content-header row">
            </div>
            <div class="content-body">
                <section class="row flexbox-container">
                    <div class="col-12 d-flex align-items-center justify-content-center">
                        <div class="col-lg-4 col-md-8 col-10 box-shadow-2 p-0">
                            <div class="card border-grey border-lighten-3 m-0">
                                <div class="card-header border-0">
                                    <div class="card-title text-center">
                                        <div class="p-1"><img src="/assets/dashboard/img/zeemoney.png" alt="Ultask logo"
                                                width="120px"></div>
                                    </div>
                                    <h6 class="card-subtitle line-on-side text-muted text-center font-small-3 pt-2">
                                        <span>Upload Required Documents</span>
                                    </h6>
                                </div>
                                <div class="card-content">

                                    <div style="margin-left: 1em; margin-right: 1em;">
                                        <%if (errors.length> 0) { %> <% errors.forEach(error=> { %>
                                                <div class="alert alert-danger alert-dismissible fade show"
                                                    role="alert">
                                                    <%- error.msg %>
                                                </div>
                                                <% }) %>
                                                    <% } %>

                                                        <%if (errorMessage) { %>
                                                            <div class="alert alert-danger alert-dismissible fade show"
                                                                role="alert">
                                                                <%- errorMessage %>
                                                            </div>
                                                            <% } %>
                                    </div>

                                    <div class="card-body">



                                        <fieldset class="form-group position-relative has-icon-left">
                                            <form class="form-horizontal form-simple" action="/login" method="POST">
                                                <h5>ID Photo</h5>
                                                <div class="row">
                                                    <div class="col-md-8">
                                                        <input type="file" name="email" class="form-control"
                                                            id="user-email" required>
                                                        <div class="form-control-position">
                                                            <i class="la la-user"></i>
                                                        </div>
                                                    </div>
                                                    <div class="col-md-4">
                                                        <button type="submit" class="btn btn-info btn-block">
                                                            Upload</button>
                                                    </div>
                                                </div>
                                            </form>
                                        </fieldset>
                                        <fieldset class="form-group position-relative has-icon-left"
                                            style="margin-top: 3em;">
                                            <h5>Proof of address</h5>
                                            <div class="row">
                                                <div class="col-md-8">
                                                    <input type="file" name="email" class="form-control" id="user-email"
                                                        required>
                                                    <div class="form-control-position">
                                                        <i class="la la-envelope"></i>
                                                    </div>
                                                </div>
                                                <div class="col-md-4">
                                                    <button type="submit" class="btn btn-info btn-block">
                                                        Upload</button>
                                                </div>
                                            </div>
                                        </fieldset>

                                        <br /><br />
                                    </div>
                                </div>

                            </div>
                        </div>
                    </div>
                </section>

            </div>
        </div>
    </div>
    <!-- END: Content-->

    <!-- BEGIN: JS-->
    <script src="/assets/dashboard/js/vendors.min.js"></script>
    <script src="/assets/dashboard/js/jquery.sticky.js"></script>
    <script src="/assets/dashboard/js/chartist.min.js"></script>
    <script src="/assets/dashboard/js/chartist-plugin-tooltip.min.js"></script>
    <script src="/assets/dashboard/js/raphael-min.js"></script>
    <script src="/assets/dashboard/js/morris.min.js"></script>
    <script src="/assets/dashboard/js/horizontal-timeline.js"></script>
    <script src="/assets/dashboard/js/app-menu.min.js"></script>
    <script src="/assets/dashboard/js/app.min.js"></script>
    <script src="/assets/dashboard/js/customizer.min.js"></script>


</body>
<!-- END: Body-->


</html>